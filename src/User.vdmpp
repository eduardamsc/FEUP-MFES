class User
	types
		public String = seq1 of char;
	
	instance variables
		public m_username: String;
		private m_password: String;
	
	operations
		public User(username: String, password: String) user: User == (
			m_username := username;
			m_password := password;
			return self;
		);
		
		public passwordMatches(password: String) matches: bool == (
			return m_password = password;
		);
		
		public getUsername() username: String == (
			return m_username;
		);
			
end User